{
  "name": "ssm",
  "version": "0.7.3",
  "description": "Command line interface for S|S|M",
  "bin": {
    "ssm": "bin/ssm",
    "ssm-install": "bin/ssm-install",
    "ssm-run": "bin/ssm-run",
    "ssm-bootstrap": "bin/ssm-bootstrap",
    "ssm-predict": "bin/ssm-predict",
    "ssm-reduce": "bin/ssm-reduce",
    "ssm-lhs": "bin/ssm-lhs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/standard-analytics/ssm.git"
  },
  "bugs": {
    "url": "https://github.com/standard-analytics/ssm/issues"
  },
  "main": "index.js",
  "preferGlobal": true,
  "scripts": {
    "install": "(cd src/C && make && make install && make clean)",
    "test": "(cd src && python test_Cmodel.py && python test_Ccoder.py) && (cd tests && make test && make clean) && ./node_modules/.bin/mocha"
  },
  "keywords": [
    "State",
    "Space",
    "Models",
    "Inference",
    "Dynamical",
    "systems",
    "Statistics"
  ],
  "author": "Sebastien Ballesteros <sebastien@standardanalytics.io>",
  "contributors": [
    {
      "name": "Joseph Dureau"
    }
  ],
  "license": "GPLv3",
  "dependencies": {
    "commander": "~2.0.0",
    "async": "~0.2.9",
    "glob": "~3.2.6",
    "minimatch": "~0.2.12",
    "mkdirp": "~0.3.5",
    "clone": "~0.1.11",
    "dpkg-squirrel": "~0.4.2",
    "lhs": "~0.0.0",
    "rmath.js": "~0.0.0",
    "seed-random": "~2.2.0",
    "tv4": "~1.0.11",
    "underscore": "~1.5.1",
    "colors": "~0.6.2",
    "rimraf": "~2.2.2",
    "binary-csv": "~0.1.6",
    "request": "~2.29.0",
    "mime": "~1.2.11",
    "dpm2": "~0.2.2",
    "data-streams": "0.0.5",
    "once": "~1.3.0"
  },
  "devDependencies": {
    "mocha": "~1.14.0"
  }
}
